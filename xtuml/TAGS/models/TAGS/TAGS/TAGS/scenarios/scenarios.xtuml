            endif

         else
            [] = RPT3:Test_Failed["Failed to delete all", Test, -20]
         endif

      else
         [] = RPT3:Test_Failed["Failed to delete all", Test, -30]
      endif

   else
      [] = RPT3:Test_Failed["Failed to delete all", Test, -40]
   endif

   Test = Test + 1

# End Test 5

#--------------------------------------------------------------------------

my_test.Number = Test',
